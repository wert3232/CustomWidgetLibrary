<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="eqBezierChart">
        <attr name="eqPointNum" format="integer"/>
        <attr name="frequencies"/>
    </declare-styleable>
    <declare-styleable name="axisView">
        <attr name="axisLength" format="integer"/>
        <attr name="axisX" format="integer"/>
        <attr name="axisY" format="integer"/>
        <attr name="pointDrawable" format="reference"/>
    </declare-styleable>
    <declare-styleable name="HorizontalWheelView">
        <attr name="marksCount" format="integer"/>
        <attr name="normalColor" format="color"/>
        <attr name="activeColor" format="color"/>
        <attr name="showActiveRange" format="boolean"/>
        <attr name="endLock" format="boolean"/>
        <attr name="onlyPositiveValues" format="boolean"/>
        <attr name="snapToMarks" format="boolean"/>
        <attr name="isCursorShow" format="boolean"/>
        <attr name="normalMarkWidth" format="integer"/>
        <attr name="startIndex" format="integer"/>
        <attr name="endIndex" format="integer"/>
        <attr name="scaleSpeedUnit" format="integer"/>
        <attr name="index" format="integer"/>
    </declare-styleable>
    <declare-styleable name="Croller">
        <attr name="progress" format="integer" />
        <attr name="label" format="string" />
        <attr name="min_val_label" format="string"/>
        <attr name="max_val_label" format="string"/>
        <attr name="val_label_size" format="dimension" />
        <attr name="val_label_color" format="color" />
        <attr name="max_val_margin_bottom_by_radiusRatio" format="float"/>
        <attr name="min_val_margin_bottom_by_radiusRatio" format="float"/>
        <attr name="max_val_margin_HorizontalCenter_by_radiusRatio" format="float"/>
        <attr name="min_val_margin_HorizontalCenter_by_radiusRatio" format="float"/>
        <attr name="back_circle_color" format="color" />
        <attr name="main_circle_color" format="color" />
        <attr name="back_circle_drawable" format="reference"/>
        <attr name="main_circle_drawable" format="reference"/>
        <attr name="main_circle_drawable_revise_degree" format="integer"/>
        <attr name="indicator_color" format="color" />
        <attr name="progress_primary_color" format="color" />
        <attr name="progress_secondary_color" format="color" />
        <attr name="label_size" format="integer" />
        <attr name="label_color" format="color" />
        <attr name="indicator_width" format="float" />
        <attr name="is_continuous" format="boolean" />
        <attr name="progress_primary_circle_size" format="float" />
        <attr name="progress_secondary_circle_size" format="float" />
        <attr name="progress_primary_stroke_width" format="float" />
        <attr name="progress_secondary_stroke_width" format="float" />
        <attr name="sweep_angle" format="integer" />
        <attr name="start_offset" format="integer" />
        <attr name="max" format="integer" />
        <attr name="min" format="integer" />
        <attr name="main_circle_radius" format="float" />
        <attr name="back_circle_radius" format="float" />
        <attr name="progress_radius" format="float" />
        <attr name="anticlockwise" format="boolean" />
    </declare-styleable>
    <declare-styleable name="selectTextView">
        <attr name="selected" format="boolean" />
    </declare-styleable>
    <declare-styleable name="VerticalSeekBar">
        <attr name="seekBarRotation">
            <!-- Clock wise - 90 deg; top = min, bottom = max -->
            <enum name="CW90" value="90" />
            <!-- Clock wise - 270 deg; bottom = min, top = max -->
            <enum name="CW270" value="270" />
        </attr>
        <attr name="onlyTouchThumbCanSlide" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="recycler_swipe_SwipeMenuLayout">
        <attr name="leftViewId" format="reference"/>
        <attr name="rightViewId" format="reference"/>
        <attr name="contentViewId" format="reference"/>
    </declare-styleable>
    <!--<declare-styleable name="RadioText">
        <attr name="viewIndex" format="integer"/>
        <attr name="selectIndex" format="integer"></attr>
    </declare-styleable>-->
    <declare-styleable name="commonAttr">
        <attr name="viewIndex" format="integer"/>
        <attr name="selectIndex" format="integer"></attr>
        <attr name="appContent" format="string"/>
        <attr name="appTextSize" format="dimension"/>
        <attr name="appTextColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="NiceSpinner">
        <attr name="arrowTint" format="color" />
        <attr name="hideArrow" format="boolean" />
        <attr name="arrowDrawable" format="reference|color"/>
        <attr name="dropDownListPaddingBottom" format="dimension"/>
        <attr name="backgroundSelector" format="reference|color"/>
        <attr name="textTint" format="color"/>
        <attr name="selectedIndex" format="integer"/>
        <attr name="entries" />
    </declare-styleable>
</resources>