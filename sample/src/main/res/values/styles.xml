<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="knob_style">
        <item name="back_circle_radius">0.93</item>
        <item name="main_circle_radius">0.86</item>
        <item name="progress_primary_color">@android:color/holo_blue_bright</item>
        <item name="progress_primary_stroke_width">10</item>
        <item name="progress_secondary_color">#6200EA</item>
        <item name="progress_secondary_stroke_width">10</item>
        <item name="indicator_color">@android:color/transparent</item>
        <item name="indicator_width">7</item>
        <item name="is_continuous">true</item>
      <!--  <item name="sweep_gradient_positions">@array/sweepGradientPositions</item>
        <item name="sweep_gradient_colors">@array/sweepGradientColors</item>-->
    </style>
    <style name="master_adjust_seek_bar_style">
        <item name="android:thumb">@drawable/thumb_p</item>
        <!--<item name="android:thumbOffset">4dp</item>-->
        <item name="android:paddingStart">100dp</item>
        <item name="android:paddingEnd">40dp</item>
        <item name="android:maxHeight">20dp</item>
        <item name="onlyTouchThumbCanSlide">true</item>
    </style>
</resources>
